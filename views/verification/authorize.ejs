<script type="text/javascript">
    $(document).on("keyup","#searchTxt",function(){
        var searchTerm = $(this).val().toLowerCase();
        $('#upload_history tbody tr').each(function(){
            var lineStr = $(this).text().toLowerCase();
            if(lineStr.indexOf(searchTerm) === -1){
                $(this).hide();
            }else{
                $(this).show();
            }
        });
    });

    $(document).on("keyup","#CompletesearchTxt",function(){
        var searchTerm = $(this).val().toLowerCase();
        $('#complete_history tbody tr').each(function(){
            var lineStr = $(this).text().toLowerCase();
            if(lineStr.indexOf(searchTerm) === -1){
                $(this).hide();
            }else{
                $(this).show();
            }
        });
    });

    $(document).on("click",".btnEmploymentDeny, .btnEducationaldenyDeny, .btnIdentificationDeny, .btnProjectDeny",function(event){
        event.preventDefault();
        ajaxindicatorstart('LOADING');
        var form = $(this).closest('form')[0];
        var formData = new FormData(form);
        var value = $(this).val();
        $.ajax({
            type: 'POST',
            url: '/authorize/allowdeny?deny='+value,
            data:  formData,
            processData: false,
            contentType: false,
            enctype: 'multipart/form-data',
            success: function (response)
            {
                location.reload();
            }
        });
        ajaxindicatorstop();
    });

    $(document).on("click",".allow_verify_request",function(event){
        event.preventDefault();
        ajaxindicatorstart('LOADING');
        var closestForm = $(this).closest("form");
        var check = closestForm.find('input[type=checkbox]:checked').length;
        if(check){
            var form = closestForm[0];
            var formData = new FormData(form);
            var value = $(this).val();
            
            $.ajax({
                type: 'POST',
                url: '/authorize/allowdeny',
                data:  formData,
                processData: false,
                contentType: false,
                enctype: 'multipart/form-data',
                success: function (response)
                {
                    if(response.status=="OK"){
                        $.bootstrapGrowl(response.msg, {type: 'success', delay: 1000});
                        location.reload();
                    }else{
                        $.bootstrapGrowl(response.msg, {type: 'danger', delay: 1000});
                    }
                }
            });
        }else{
            $.bootstrapGrowl("Please select atleast one checkbox", {type: 'danger', delay: 1000});
        }
        ajaxindicatorstop();

        // ajaxindicatorstart('LOADING');
        // event.preventDefault();
        // var closestForm = $(this).closest("form");
        // var check = closestForm.find('input[type=checkbox]:checked').length;
        // if(check){
        //     closestForm.submit();
        // }else{
        //     $.bootstrapGrowl("Please select atleast one checkbox", {type: 'danger', delay: 1000});
        //     return false;
        // }
        // ajaxindicatorstop();
    });


</script>