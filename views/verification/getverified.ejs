function store_verify_data(formData,ajax_url,redirect_url){
     ajaxindicatorstart('LOADING');
    $.ajax({
        url: ajax_url,
        method: 'POST',
        data:  formData,
        mimeType:"multipart/form-data",
        processData: false,
        contentType: false,
        enctype: 'multipart/form-data',
        success: function(results)
        {
            var json_obj = jQuery.parseJSON( results );
            if(json_obj.status=='OK'){
                $.bootstrapGrowl('Record save successfully', {type: 'success', delay: 1000});
                setTimeout(function() {
                    ajaxindicatorstop();
                    var current_url = window.location.pathname;
                    window.location = current_url + redirect_url;
                    location.reload();
                }, 3500);

            }else{
                ajaxindicatorstop();
                $.bootstrapGrowl(json_obj.msg, {type: 'danger', delay: 1500});
            }
        }
    });
}
