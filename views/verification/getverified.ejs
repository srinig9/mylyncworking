<style>
#parent #popup { display: none; }
#parent:hover #popup { display: block; }
</style>
<div class="main-container">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9 pull-right">
                <div class="p7_main_container">
                    <div class="p7_201">
                        <div class="tabs_panel">
                            <% if(typeof req.user.company_id!='undefined' && req.user.company_id!=''){%>
                            <% } else {%>
                            <ul class="nav nav-tabs" role="tablist">
                                <li class="active"><a href="#contact" aria-controls="Contact" role="tab" data-toggle="tab">Contacts</a></li>
                                <li><a href="#identification" aria-controls="Identification" role="tab" data-toggle="tab">Identity</a></li>
                                <li><a href="#education" aria-controls="Education" role="tab" data-toggle="tab">Education</a></li>
                                <li><a href="#experience" aria-controls="Experience" role="tab" data-toggle="tab">Experience</a></li>
                                <li><a href="#project" aria-controls="Project" role="tab" data-toggle="tab">Projects</a></li>
                                <li><a href="#other_docs" aria-controls="Other_docs" role="tab" data-toggle="tab">Other Docs</a></li>
                            </ul>
                            <% } %>
                            <!-- Tab panes -->
                            <div class="tab-content">
								<!-- Contact Data tab start -->
                                <div role="tabpanel" class="tab-pane active" id="contact">
                                    <div class="p7_i102">
                                        <div class="page_section_title p7_section_title">
                                            <h3>Contact Info</h3>
                                        </div>
                                        <div class="p7_section_content">
                                            <div class="row no-margin">
                                                <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8">
                                                    <div class="form-group">
                                                        <label class="label" for="">Email</label>
                                                        <%
                                                            var readonly='';
                                                            var email_val='';
                                                            var email_btn='Verify';
                                                            var phone_val='';
                                                            var phone_read='';
                                                            var phone_btn = 'Verify';
                                                            var dial_disable = '';
                                                            var is_email_verify=0;
                                                            var is_phone_verify=0;

                                                            if(typeof userverify!='undefined'){
                                                                if(typeof userverify.email!='undefined' && userverify.email!=''){
                                                                    email_val = userverify.email;
                                                                    if(userverify.is_email_verify==1){
                                                                        readonly = '';
                                                                        is_email_verify=1;
                                                                    }
                                                                    email_btn = 'Verify';
                                                                }
                                                                if(typeof userverify.phone!='undefined' && userverify.phone!=''){
                                                                    phone_val = userverify.phone;
                                                                    phone_btn = 'Verify';
                                                                    if(userverify.is_phone_verify==1){
                                                                        is_phone_verify=1;
                                                                    }
                                                                }
                                                            }
                                                            %>
                                                        <input type="hidden" value="<%=is_email_verify%>" name="verify_email_flag" id="verify_email_flag">

                                                        <input type="email" name="email" <%=readonly%> id="verify_email" placeholder="Email" class="p7_input form-control email_verify_field" value="<%=email_val%>" required="required">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                                                    <p>&nbsp;</p>
                                                    <% if(typeof userverify!='undefined' && typeof userverify.is_email_verify!='undefined' && userverify.is_email_verify==0) {%>
                                                        <a href="javscript:;" id="verify_email_btn" class="btn btn-primary btn-primary-b30"><%=email_btn%></a> &nbsp; <a href="javascipt:;" data-target="#email_otpModal" data-toggle="modal" class="text_underline">I have OTP</a> &nbsp;
                                                    <% } else if(typeof userverify!='undefined' && typeof userverify.is_email_verify!='undefined' && userverify.is_email_verify==1) { %>
                                                        <span class="green-checked email_verify_check"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
                                                        <a href="javscript:;" id="verify_email_reset" style="display:none;" class="btn btn-primary show_verify_button btn-primary-b30"><%=email_btn%></a>
                                                    <% }else{ %>
                                                        <a href="javscript:;" id="verify_email_btn" class="btn btn-primary btn-primary-b30"><%=email_btn%></a>
                                                        &nbsp; <a href="javascipt:;" data-target="#email_otpModal" data-toggle="modal" class="text_underline">I have OTP</a> &nbsp;
                                                    <% } %>
                                                </div>
                                            </div>
                                            <div class="row no-margin">
                                                <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8">
                                                    <div class="form-group">
                                                        <label class="label" for="">Phone</label><br>
                                                        <div class="row">
                                                        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
                                                        <select <%=dial_disable%> class="form-control" name="dial_code" id="dial_code">
                                                            <option value="">Country Code</option>
                                                            <%
                                                            if(typeof dial_code!='undefined' && dial_code.length>0){
                                                                _.each(dial_code,function(code){ 
                                                                    var selected='';
                                                                    var dial_final = '';
                                                                    if(typeof userverify!='undefined' && typeof userverify.dial_code!='undefined' && userverify.dial_code==code.dial_code){
                                                                    selected = 'selected'; 
                                                                }
                                                                //dial_final = code.dial_code.replace("+", "");
                                                                dial_final = code.name + " (" + code.dial_code + ")";
                                                            %>
                                                            <option <%=selected%> value="<%=code.dial_code%>"><%=dial_final%></option>
                                                            <% }) 
                                                            }%>
                                                        </select>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-6 col-md-8 col-lg-8">
                                                        <input type="hidden" value="<%=is_phone_verify%>" name="verify_phone_flag" id="verify_phone_flag">
                                                        <input  type="tel" name="phone" id="verify_phone" placeholder="Phone" <%=phone_read%> class="p7_input form-control phone_verify_field" value="<%=phone_val%>" required="required">
                                                        </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                                                    <p>&nbsp;</p>
                                                    <% if(typeof userverify!='undefined' && typeof userverify.is_phone_verify!='undefined' && userverify.is_phone_verify==0) {%>
                                                        <a href="javascript:;" id="verify_phone_btn" class="btn btn-primary btn-primary-b30"><%=phone_btn%></a> &nbsp; 
                                                        <a href="javascipt:;" data-target="#otpModal" data-toggle="modal" class="text_underline">I have OTP</a>
                                                        &nbsp;
                                                    <% } else if(typeof userverify!='undefined' && typeof userverify.is_phone_verify!='undefined' && userverify.is_phone_verify==1) { %>
                                                        <span class="green-checked phone_verify_check"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
                                                        <a href="javascript:;" id="verify_phone_reset" class="btn btn-primary show_verify_phone_button btn-primary-b30"><%=phone_btn%></a> &nbsp;
                                                    <% }else{%>
                                                        <a href="javascript:;" id="verify_phone_btn" class="btn btn-primary btn-primary-b30"><%=phone_btn%></a> &nbsp; 
                                                        <a href="javascipt:;" data-target="#otpModal" data-toggle="modal" class="text_underline">I have OTP</a>
                                                        &nbsp;
                                                    <% } %>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Contact Data tab end -->

								<!-- Identification Data tab start -->
                                <div role="tabpanel" class="tab-pane" id="identification">
                                    <!-- <div class="p7_i101">
										<ul class="blue_dots">
                                            <li>All documents must be high resolution, color image files.</li>
                                            <li>Allowed file type extensions are: JPEG, JPG, PNG.</li>
                                            <li>Files must be at least 98KB and at most 3MB.</li>
                                            <li>Image dimensions can be a minimum of 350px x 350px and maximum of 4096px x 4096px.</li>
                                        </ul>
                                    </div> -->
                                    <div class="p7_i102">
                                        <div class="page_section_title p7_section_title">
                                             <h3>Personal Information</h3>
                                            <span style="margin-left: 100px;" class="pull-right">&nbsp; &nbsp;
                                            <button type="button" class="btn btn-primary btn-primary-sw identity_popup" id="identity_info">Add Identity</button>&nbsp; &nbsp;&nbsp;&nbsp;<button type="button" class="btn btn-primary btn-primary-sw identity_popup" id="identity_dob">Add Date of Birth</button>&nbsp; &nbsp;&nbsp;
                                            <button type="button" class="btn btn-primary btn-primary-sw identity_popup" id="identity_address">Add Address</button>
                                            </span>
                                        </div>
                                        <div class="p7_section_content">
                                            <div class="p6_pro_1000">
                                                <div class="p6_section_content p6_project_li">
                                                <% if(typeof userVerifyDatas!='undefined' && userVerifyDatas.length>0){
                                                        _.each(userVerifyDatas,function(identity){
                                                            if(typeof identity.legal_name!='undefined' || typeof identity.first_name!='undefined' || identity.last_name!='undefined'){

                                                            var legal_name='';
                                                            var first_name= '';
                                                            var last_name = '';
                                                            if(typeof identity.legal_name!='undefined' && identity.legal_name!=''){
                                                                legal_name = identity.legal_name; 
                                                            }
                                                            if(typeof identity.first_name!='undefined' && identity.first_name!=''){
                                                                first_name = identity.first_name; 
                                                            }

                                                            if(typeof identity.last_name!='undefined' && identity.last_name!=''){
                                                                last_name = identity.last_name; 
                                                            }

                                                            if(first_name!='' || last_name!=''){
                                                                legal_name = first_name+' '+last_name;
                                                            }

                                                    %> 
                                                    <div class="p6_sc_1001">
                                                        <a class="p6_sc_1002 project_img" href="/profile" target="_blank"><span class="first-letter">
                                                            <%=legal_name.charAt(0).toUpperCase()%>
                                                        </span></a>
                                                        <div class="p6_sc_1003">
                                                            <div class="p6_sc_1004 designation"><a href="/profile"><%=legal_name%></a>
                                                                 <div class="user-profile-verification">
                                                                     <a href="javascript:;" class="show_verify_data_popup action_recomm_111" data-id="<%=identity.id%>">Get Verified</a>
                                                                        <%
                                                                        if(typeof identity.verify_request_id!='undefined' && identity.verify_request_id.length>0){
                                                                            var verifyUserData = '';
                                                                            verifyUserData = identity.verify_request_id;
                                                                            var count=0;
                                                                            var count2=0;

                                                                            _.each(verifyUserData,function(vdata){ 
                                                                                var verify_flag=0;
                                                                                if(typeof vdata.txnno!='undefined' && vdata.txnno!=''){
                                                                                    verify_flag=1;
                                                                                }

                                                                                if(typeof vdata.user_id!='undefined'){
                                                                                    var UserImage = '/themes/frontend/images/default-user.png';
                                                                                    if(typeof vdata.user_id.profile_image!='undefined' && vdata.user_id.profile_image!=''){
                                                                                        UserImage = sails.config.appUrlwPort+'/uploads/users/'+vdata.user_id.profile_image;
                                                                                    }
                    
                                                                                    var user_slug = '/';
                                                                                    if(typeof vdata.user_id.slug!='undefined' && vdata.user_id.slug!=''){
                                                                                        user_slug = '/profile/'+vdata.user_id.slug;
                                                                                    }

                                                                        %>
                                                                    <% if(verify_flag==1){
                                                                            count=count+1;
                                                                          if(count==1){%>
                                                                            <span class="checked"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
                                                                            <span style="font-size: 12px; font-weight: normal;">Verified by </span>
                                                                            <% } %>
                                                                                <a href="<%=user_slug%>" title="<%=vdata.user_id.name%>"><img src="<%=UserImage%>" width="19" height="19" class="img-circle"></a>
                                                                            
                                                                            <% }else{ 
                                                                                count2=count2+1;
                                                                                %>
                                                                                <% if(count2==1){%>
                                                                                    &nbsp;&nbsp;<span style="font-size: 12px; font-weight: normal;">Requested to </span>
                                                                                    <% } %>
                                                                                    <a href="<%=user_slug%>" title="<%=vdata.user_id.name%>"><img src="<%=UserImage%>" width="19" height="19" class="img-circle"></a>
                                                                                <% } 

                                                                                 }
                                                                            });%>
                                                                    <% }%>
                                                                </div>
                                                            </div>
                                                            <% 
                                                                var birthdate='';
                                                                var doc_country=0;
                                                                var issue_dates=0;
                                                                if(typeof identity.dob!='undefined' && identity.dob!=''){
                                                                    birthdate = identity.dob;
                                                                }
                                                            %>
                                                            <% if(typeof identity.address_line_1!='undefined' && identity.address_line_1!=''){%>
                                                                <div class="p6_1006 period">Address : <%=identity.address_line_1%></div>
                                                            <% } %>

                                                            <% if(typeof identity.city!='undefined' && identity.city!=''){%>
                                                                <div class="p6_1006 period">City : <%=identity.city%></div>
                                                            <% } %>
                                                            
                                                            <% 
                                                            var passport_flag=0
                                                            if(typeof identity.issue_place!='undefined' && identity.issue_place!=''){
                                                                passport_flag=1;
                                                            }
                                                            if(typeof identity.doc_country!='undefined' && typeof identity.doc_country.name!='undefined' && identity.doc_country.name!='' && passport_flag==0){
                                                            doc_country=1;
                                                            %>
                                                                <div class="p6_1006 period">Country : <%=identity.doc_country.name%></div>
                                                            <% } %>

                                                            <% if(typeof identity.issue_country!='undefined' && identity.issue_country!='' && doc_country==0){
                                                            var label = 'Country';
                                                            if(passport_flag==1){
                                                                label = 'Issuing Country';
                                                            }
                                                            %>
                                                                <div class="p6_1006 period"><%=label%> : <%=identity.issue_country%></div>
                                                            <% } %>

                                                            <% if(typeof identity.country!='undefined' && identity.country!='' && doc_country==0){%>
                                                                <div class="p6_1006 period">Country : <%=identity.country%></div>
                                                            <% } %>
                                                            
                                                             <% if(typeof identity.issue_date!='undefined' && identity.issue_date!=''){
                                                             issue_dates=1;
                                                                var issueDate='';
                                                                issueDate =identity.issue_date;
                                                                
                                                                if(issueDate.length<11){
                                                                    issueDate =issueDate.replace("/", "-");
                                                                    issueDate = moment(issueDate,'DD-MM-YYYY').format('DD-MMM-YYYY');
                                                                }
                                                             %>
                                                                <div class="p6_1006 period">Issuance Date : <%=issueDate%></div>
                                                            <% } %>

                                                            <% if(birthdate!='' && issue_dates==0){
                                                                var bdate='';

                                                                 if(birthdate.length<11){
                                                                    birthdate=birthdate.replace("/", "-");
                                                                    bdate = moment(birthdate,'DD-MM-YYYY').format('DD-MMM-YYYY');
                                                                }else{
                                                                        bdate = birthdate;
                                                                }
                                                            %>
                                                                <div class="p6_1006 period">Dob : <%=bdate%></div>
                                                            <% } %>
                                                           
                                                            <% if(typeof identity.doc_type_id!='undefined' && typeof identity.doc_type_id.title!='undefined'){
                                                            var doc_title = '';
                                                            doc_title = identity.doc_type_id.title+' No : ';
                                                            if(typeof identity.doc_num!='undefined' && identity.doc_num!=''){
                                                            %>
                                                                <div class="p6_1006 period"><%=doc_title%><%=identity.doc_num%></div>
                                                            <% }
                                                            } %>

                                                            <% if(typeof identity.birth_place!='undefined' && identity.birth_place!=''){%>
                                                                <div class="p6_1006 period">Birth Place : <%=identity.birth_place%></div>
                                                            <% } %>
                                                        </div>
                                                        <button type="button" class="p6_sc_1099" onclick="edit_identity_popup('<%=identity.id%>')"><i class="fa fa-edit" aria-hidden="true"></i> </button>
                                                    </div>
                                                    <%   }
                                                        });
                                                    }else{ %>
                                                    <div class="p6_sc_1001"> No Identity added yet!</div>
                                                    <% }%>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
								<!-- Identification Data tab end -->
								
								<!-- Education Data tab start -->
                                <div role="tabpanel" class="tab-pane" id="education">
                                    <div class="p7_i102">
                                        <div class="page_section_title p7_section_title">
                                            <h3>Education</h3>
                                            <button type="button" class="btn btn-primary btn-primary-sw pull-right educations_popup" id="add">Add</button>
                                        </div>
                                        <div class="p7_section_content">
                                            <div class="p6_pro_1000">
                                                <div class="p6_section_content p6_edu_li">
                                                <% 
                                                if(typeof userInfo!='undefined' && userInfo.length>0){
                                                    for(var i=0; i<userInfo.length; i++) {
                                                        var user_info = '';
                                                        user_info = userInfo[i];
                                                    %>
                                                    <div class="p6_sc_1001 education_verify_block">
                                                    <a class="p6_sc_1002 project_img" href="" target="_blank"><img src="<%=sails.config.appUrlwPort%>/themes/frontend/images/education_icon.png" alt="Education" height="70" width="70" /></a>
                                                        <div class="p6_sc_1003">
                                                            <div class="p6_sc_1004 designation"><%=user_info.school%>
                                                                <div class="user-profile-verification">
                                                                        <% if(typeof user_info.educationdocs!='undefined'){
                                                                            if(user_info.educationdocs.length>0){
                                                                                %>
                                                                                <a href="javascript:;" class="show_verify_data_popup action_recomm_111" data-id="<%=user_info.educationdocs[0].id%>">Get Verified</a>
                                                                                <%
                                                                                if(typeof user_info.educationdocs[0].verify_request_id!='undefined' && user_info.educationdocs[0].verify_request_id.length>0){
                                                                                    var verifyUserData = '';
                                                                                    verifyUserData = user_info.educationdocs[0].verify_request_id;
                                                                                    var count=0;
                                                                                    var count2=0;
                                                                                    _.each(verifyUserData,function(vdata){ 
                                                                                        var verify_flag=0;
                                                                                        if(typeof vdata.txnno!='undefined' && vdata.txnno!=''){
                                                                                            verify_flag=1;
                                                                                        }
                                                                                        count=count+1;
                                                                                        if(typeof vdata.user_id!='undefined'){
                                                                                            var UserImage = '/themes/frontend/images/default-user.png';
                                                                                            if(typeof vdata.user_id.profile_image!='undefined' && vdata.user_id.profile_image!=''){
                                                                                                UserImage = sails.config.appUrlwPort+'/uploads/users/'+vdata.user_id.profile_image;
                                                                                            }
                            
                                                                                            var user_slug = '/';
                                                                                            if(typeof vdata.user_id.slug!='undefined' && vdata.user_id.slug!=''){
                                                                                                user_slug = '/profile/'+vdata.user_id.slug;
                                                                                            }
                                                                                    %>
                                                                                    <% if(verify_flag==1){ %>
                                                                                    <% if(count==1){%>
                                                                                    <span class="checked"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
                                                                                    <span style="font-size: 12px; font-weight: normal;">Verified by </span>
                                                                                    <% } %>
                                                                                        <a href="<%=user_slug%>" title="<%=vdata.user_id.name%>"><img src="<%=UserImage%>" width="19" height="19" class="img-circle"></a>
                                                                                    
                                                                                    <% }else{ 
                                                                                        count2=count2+1;
                                                                                        %>
                                                                                        <% if(count2==1){%>
                                                                                            &nbsp;&nbsp;<span style="font-size: 12px; font-weight: normal;">Requested to </span>
                                                                                            <% } %>
                                                                                            <a href="<%=user_slug%>" title="<%=vdata.user_id.name%>"><img src="<%=UserImage%>" width="19" height="19" class="img-circle"></a>
                                                                                        <% } %>
                                                                                        <%
                                                                                        }
                                                                                    });
                                                                                }
                                                                            }
                                                                        }%>
                                                                    </div>
                                                            </div>
                                                            <div class="p6_1005 Branch">
                                                                <%
                                                                    var degree_field='';
                                                                    if(typeof user_info.degree!='undefined' && user_info.degree!=''){
                                                                        degree_field+=user_info.degree; 
                                                                    }

                                                                    if(typeof user_info.study_field!='undefined' && user_info.study_field!=''){
                                                                        if(degree_field!=''){
                                                                        degree_field+=', '+user_info.study_field;
                                                                        }else{
                                                                            degree_field+=user_info.study_field;
                                                                        }
                                                                    }
                                                                %>
                                                                <%=degree_field%>
                                                            </div>
                                                            <div class="p6_1006 period">
                                                            <%
                                                                var from_year   ='';
                                                                var to_year     ='';
                                                                if(typeof user_info.from_year!='undefined' && user_info.from_year!=''){
                                                                    from_year=user_info.from_year;
                                                                }
                                                                if(typeof user_info.to_year!='undefined' && user_info.to_year!=''){
                                                                    to_year=user_info.to_year;
                                                                }
                                                            %>
                                                            <% if(from_year!=''){ %> <%=from_year%> 
                                                            <% } %>
                                                            <% if(to_year!=''){ %>- <%=to_year%>
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                    <button type="button" onclick="edit_education('<%=user_info.id%>')" class="p6_sc_1099"><i class="fa fa-edit" aria-hidden="true"></i> </button>
                                                </div>
                                                <% }
                                                }else{ %>
                                                <div class="p6_sc_1001"> No Education added yet!</div>
                                                <% }%>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
								<!-- Education Data tab end -->
								
								<!-- Experience Data tab start -->
                                <div role="tabpanel" class="tab-pane" id="experience">
                                    <!-- <div class="p7_i101">
                                        <ul class="blue_dots">
                                            <li>All documents must be high resolution, color image files.</li>
                                            <li>Allowed file type extensions are: JPEG, JPG, PNG.</li>
                                            <li>Files must be at least 98KB and at most 3MB.</li>
                                            <li>Image dimensions can be a minimum of 350px x 350px and maximum of 4096px x 4096px.</li>
                                            <li>Proof of residence document must contain name, address and be dated less than 3 months ago.</li>
                                            <li>For languages such as Russian, Chinese, Thai, Hebrew, etc. that use non-Latin characters a notrized translation of your proof of residence document will greatly speed up processing time. Japanese documents do not required a notarized translation.</li>
                                        </ul>
                                    </div> -->
                                    <div class="p7_i103">
                                        <div class="page_section_title p7_section_title">
                                            <h3>Experience</h3>
                                            <button type="button" class="btn btn-primary btn-primary-sw pull-right experience_popup" id="add">Add</button>
                                        </div>
                                        <div class="p7_section_content">
                                                <div class="p6_pro_1000">
                                                        <div class="p6_section_content p6_exp_li">
                                                        <% 
                                                        if(typeof userexp!='undefined' && userexp.length>0){
                                                            for(var i=0; i<userexp.length; i++) {
                                                                var company_name="";
                                                                var image_letter='';
                                                                var user_data='';
                                                                user_data = userexp[i]; 
                                                                if(typeof user_data.company_id!='undefined' && typeof user_data.company_id.company_name!="undefined"){
                                                                        company_name=user_data.company_id.company_name;
                                                                }
                                                                if(company_name!=''){
                                                                    image_letter=company_name;
                                                                }else{
                                                                    image_letter=user_data.title;
                                                                }
                                
                                                            %>
                                                            <div class="p6_sc_1001 experience_verify_block">
                                                                <a class="p6_sc_1002 project_img" href="" target="_blank"><span class="first-letter">
                                                                <%=image_letter.charAt(0).toUpperCase()%>
