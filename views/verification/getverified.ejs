                issueCountry=$.trim($(form_id+' #passport_identity_country').val());
                dob         =$.trim($(form_id+' #identity_dob_date').val())+'-'+$.trim($(form_id+' #identity_dob_month').val())+'-'+$.trim($(form_id+' #identity_dob_year').val());
            }
        
            if(address_flag==1){
                docType='';
                country=$.trim($(form_id+' #identity_country').val());
            }
            if(dob_flag==1){
                birth_place =$.trim($(form_id+' #birth_place').val());
                docType     ='';
                dob         =$.trim($(form_id+' #dob_date').val())+'-'+$.trim($(form_id+' #dob_month').val())+'-'+$.trim($(form_id+' #dob_year').val());
