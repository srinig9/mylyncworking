            var address_1   =$.trim($(form_id+' #addressline1').val());
            var address_2   =$.trim($(form_id+' #addressline2').val());
            var city        =$.trim($(form_id+' #locality').val());
            var state       =$.trim($(form_id+' #administrative_area_level_1').val());
            var zip         =$.trim($(form_id+' #zipcode').val());
            var doc_country =$.trim($(form_id+' #doc_country').val());
            var gender      =$.trim($(form_id+' #identity_gender').val());
            name_str        = legal_name;

            if(first_name!='' || last_name!=''){
                name_str=first_name+last_name;
            }else{
                name_str=legal_name;
            }

            new_string = docType+name_str+doc_num+issueCountry+issue_place+issue_date;
            new_string+= expiry_date+dob+birth_place+address_1+address_2+city+state;
            new_string+= country+zip+doc_country+gender+comment+docUrl;

            //console.log('OLD==>'+old_string);
            //console.log('NEW==>'+new_string);
            
            if($(form_id+" #is_verify_flag").val()==1){
                if(old_string!=new_string){
                    $.confirm({content:"Any verification on this record will be lost. Do you want to continue edit?",
                        header: "Confirmation",
                        confirm:function () {
                            
                        },
                        cancel:function () {
                            return false;
                        }
                    });                    
                    // if (confirm('Any verification on this record will be lost. Do you want to continue edit?')) {
                    // }else{
                    //     return false;
                    // }
                }
            }
        }
        else{

            //add time check date validation
            if($(form_id+' #identity_info_val').is(':checked')){
                var check_dob='';
                var issuedate='';
                var expirydate='';

                check_dob  =$.trim($(form_id+' #identity_dob_month').val())+'-'+$.trim($(form_id+' #identity_dob_date').val())+'-'+$.trim($(form_id+' #identity_dob_year').val());
            
                issuedate  =$.trim($(form_id+' #issuedate').val());
                expirydate  =$.trim($(form_id+' #expirydate').val());
                if(issuedate!=''){
                    issuedate = moment(issuedate).format('MM-DD-YYYY');
                }
                if(expirydate!=''){
                    expirydate = moment(expirydate).format('MM-DD-YYYY');
                }

                if(Getverify_Checkdate(check_dob,issuedate,expirydate,msg=1)==false){
                    return false;
                }
            }
        }

        var redirect_url= "#identification";
        store_verify_data(formData,ajax_url,redirect_url);
    });

