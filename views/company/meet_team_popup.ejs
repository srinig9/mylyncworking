<%
    var sengment = req.url;
    var display_flag=1;
    if(sengment=='/profile/administration'){
    	display_flag=0;
	}
%>
<div id="meet_team_member" class="modal fade" role="dialog">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4 class="modal-title team_member_title">Add Team Member</h4>
				</div>
				<form method="post" class="form-group" id="form_team_member">
				<input type="hidden" name="id" id="team_id" value="">	
				<div class="modal-body">
					<div id="form-errors"></div>
					<div class="row">
						<div class="col-md-12">
							<div class="form-group">
								<%
										var dataResult = "C";
										if(req.user.company_id){
												dataResult = "";
										}
								%>
								<input type="hidden" name="display_status" id="display_status" value="<%=display_flag%>">
								<input type="hidden" name="user_id" id="search_user_id" placeholder="User ID">
								<input type="text" class="form-control search" id="searchUserbox" value="" placeholder="Search User" data-result="<%=dataResult%>" autocomplete="off"/>
								<div id="displaysearchUsers"></div>
							</div>
							
							<div class="form-group">
								<div class="checkbox">
								  <label><input type="checkbox" name="super_user" id="super_user" value="1">&nbsp;Super User</label>
									&nbsp;&nbsp;&nbsp;
								  <label><input type="checkbox" name="user_admin" id="user_admin" value="1">&nbsp;User Admin</label>
									&nbsp;&nbsp;&nbsp;
								  <label><input type="checkbox" name="allow_verify" id="allow_verify" value="1">&nbsp;Allow Verify</label>
									&nbsp;&nbsp;&nbsp;
								  <label><input type="checkbox" name="allow_job_post" id="allow_job_post" value="1">&nbsp;Allow Job Post</label>
									&nbsp;&nbsp;&nbsp;
								  <label><input type="checkbox" name="download" id="download" value="1">&nbsp;Allow Download</label>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button type="submit" class="btn btn-primary mBottom0">Submit</button>
					<button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
				</div>
			</div>
			</form>
		</div>
	</div>