				  },
				  768: {
					items: 3
				  },
				  1200: {
					items: 4
				  }
				}
			});
			$("#feed-slider2").owlCarousel({
				margin: 0,
				loop: false,
				nav:true, 
				dots: false,
				navText:  [
				"<i class='fa fa-angle-left'></i>",
				"<i class='fa fa-angle-right'></i>"],  
				responsive: {
