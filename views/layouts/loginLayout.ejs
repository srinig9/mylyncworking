			$(".form-block-signup").hide();
			$(".form-block-signinqr").hide();
			$('.loginswitch a').click(function(){
				if($(this).parents('.block-signup').length>0) {
					$('.form-block-signinqr').fadeOut( "slow", function() {
						$('.form-block-signin').fadeIn("slow");
					});
				} else if($(this).parents('.form-block-signup').length>0) {
					$('.form-block-signup').fadeOut( "slow", function() {
						$('.form-block-signin').fadeIn("slow");
					});
				} else if($(this).parents('.form-block-signinqr').length>0) {
					$('.form-block-signinqr').fadeOut( "slow", function() {
						$('.form-block-signup').fadeIn("slow");
						//$('.form-block-signin').fadeIn("slow");
					});
				} else if($(this).parents('.form-block-signin').length>0){
					$('.form-block-signin').fadeOut( "slow", function() {
						$('.form-block-signinqr').fadeIn("slow");
					});
				}
			});
			$('.loginswitch a.skip-qr').click(function(){
				if($(this).parents('.form-block-signin').length>0){
					$('.form-block-signin').fadeOut( "slow", function() {
						$('.form-block-signup').fadeIn("slow");
					});
				} else if($(this).parents('.form-block-signup').length>0) {
					$('.form-block-signup').fadeOut( "slow", function() {
						$('.form-block-signin').fadeIn("slow");
					});
				}
			});
		});
	</script>
		<% if(typeof req.query!='undefined' && typeof req.query.ref!='undefined' && req.query.ref!=''){ %>
